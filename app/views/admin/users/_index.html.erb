<table class="certificates table">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Role</th>
    <th>Actions</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.account_type %></td>

      <td>
        <%= link_to(
          "edit", edit_admin_user_path(user)
        ) %>

    </td>
    </tr>
  <% end %>
</table>
